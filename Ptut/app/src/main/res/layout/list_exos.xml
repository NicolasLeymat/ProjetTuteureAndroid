<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/TitreSuivi"
        android:layout_width="265dp"
        android:layout_height="42dp"
        android:layout_marginStart="8dp"
        android:text="Nom de l'exercice"
        android:textColor="@color/black"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints, HardcodedText"
        tools:layout_editor_absoluteY="32dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="382dp"
        android:layout_height="56dp"
        android:autofillHints="your hint here"
        android:inputType="textPersonName"
        android:text="Saisir le nom de l'exercice"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.618"
        app:layout_constraintStart_toEndOf="@+id/TitreSuivi"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints"
        tools:layout_editor_absoluteY="32dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Nombre de répétition"
        android:textColor="@color/black"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints, HardcodedText"
        tools:layout_editor_absoluteY="131dp" />

    <!--suppress AndroidUnknownAttribute -->
    <EditText
        android:id="@+id/editTextNumber"
        android:layout_width="394dp"
        android:layout_height="55dp"
        android:layout_marginStart="48dp"
        android:ems="10"
        android:inputType="number"
        android:text="Saisir le nombre de repetition"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints"
        tools:layout_editor_absoluteY="123dp"
         />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="135dp"
        android:layout_height="31dp"
        android:layout_marginStart="8dp"
        android:text="Durée"
        android:textColor="@color/black"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints, HardcodedText"
        tools:layout_editor_absoluteY="217dp" />

    <EditText
        android:id="@+id/editTextTime"
        android:layout_width="214dp"
        android:layout_height="55dp"
        android:layout_marginStart="172dp"
        android:autofillHints="o"
        android:ems="10"
        android:inputType="time"
        android:text="Saisir la duree"
        android:textColor="@color/black"
        android:textSize="28sp"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints"
        tools:layout_editor_absoluteX="316dp"
        tools:layout_editor_absoluteY="211dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="Puissance"
        android:textColor="@color/black"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints, HardcodedText"
        tools:layout_editor_absoluteY="298dp" />

    <EditText
        android:id="@+id/editTextNumber3"
        android:layout_width="329dp"
        android:layout_height="51dp"
        android:autofillHints=""
        android:ems="10"
        android:inputType="number"
        android:text="Saisir la puissance"
        android:textColor="@color/black"
        android:textSize="30sp"
        tools:ignore="HardcodedText,LabelFor,MissingConstraints"
        tools:layout_editor_absoluteX="322dp"
        tools:layout_editor_absoluteY="293dp" />



</androidx.constraintlayout.widget.ConstraintLayout>